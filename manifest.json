{
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 3,
  "name": "AI Filler for Google Forms",
  "version": "2.1.0",
  "description": "Automatically detects and answers all question types on Google Forms by querying an AI server.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://google-form-server.onrender.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/forms/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ]
}